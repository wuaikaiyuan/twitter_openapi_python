{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      },
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}